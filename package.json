{
  "name": "redux-react-boilerplate",
  "version": "0.1.1",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "bootstrap": "^4.0.0",
    "lodash": "^4.17.5",
    "moment": "^2.21.0",
    "node-sass-chokidar": "^1.2.1",
    "npm-run-all": "^4.1.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-facebook-login-component": "^0.9.2",
    "react-feather": "^1.0.8",
    "react-google-login-component": "^0.9.3",
    "react-infinite-scroller": "^1.1.4",
    "react-modal-login": "^1.2.0",
    "react-redux": "^5.0.7",
    "react-redux-loading-bar": "^3.1.2",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.1",
    "reactstrap": "^5.0.0-beta.3",
    "recompose": "^0.26.0",
    "redux": "^3.7.2",
    "redux-form": "^7.3.0",
    "redux-reactstrap-modal": "^1.0.3",
    "redux-thunk": "^2.2.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src/styles/scss/ --include-path ./node_modules src/styles/scss/ -o src/styles/css/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src/styles/scss/ --include-path ./node_modules src/styles/scss/ -o src/styles/css/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
